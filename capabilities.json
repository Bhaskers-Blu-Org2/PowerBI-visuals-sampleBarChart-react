{
  "dataRoles": [
    {
      "name": "category",
      "displayName": "Category Data",
      "kind": "Grouping"
    },
    {
      "name": "values",
      "displayName": "Values",
      "kind": "Measure"
    },
    {
        "displayName": "Tooltips",
        "name": "Tooltips",
        "kind": "Measure"
    }
  ],
  "dataViewMappings": [{
    "conditions": [{
        "category": {
            "max": 1
        },
        "values": {
            "max": 1
        }
    }],
    "categorical": {
        "categories": {
            "for": {
                "in": "category"
            }
        },
        "values": {
            "select": [{
                "bind": {
                    "to": "values"
                }
            }]
        }
    }
  }],
  "objects": {
  }
}